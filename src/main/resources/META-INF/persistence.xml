<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
  version="2.0">
  <persistence-unit name="hellodeltaspike" transaction-type="JTA">
    <properties>
      <property name="hibernate.ejb.cfgfile" value="hellodeltaspike.cfg.xml"/>
    </properties>
  </persistence-unit>
  <persistence-unit name="hellodeltaspike2" transaction-type="JTA">
    <properties>
      <property name="hibernate.ejb.cfgfile" value="hellodeltaspike2.cfg.xml"/>
    </properties>
  </persistence-unit>
</persistence>
